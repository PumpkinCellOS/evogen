function is_prime(n)
{
    if(n < 2) return false;
    // convert to number
    n = -(-n);
    let i = 2;
    let c = 0;
    while(i < n)
    {
        if(n % i == 0)
            c++;
        i++;
    }
    return c == 0;
}

let i = 2000;
let start = new Time();
while(i < 2400) sys.writeln(i, ": ", is_prime(i++));
sys.writeln("Time: " + (new Time() - start) + "s");
